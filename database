-- ----------------------------
-- Table structure for `shop_goodscomment`
-- ----------------------------
DROP TABLE IF EXISTS `shop_goodscomment`;
CREATE TABLE `shop_goodscomment` (
  `goods_gid` int(11) NOT NULL,
  `user_uid` int(11) NOT NULL,
  `content` varchar(255) NOT NULL,
  `createtime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `gcommentid` int(11) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`gcommentid`),
  KEY `goods_gc_fk` (`goods_gid`),
  KEY `user_gc_fk` (`user_uid`),
  CONSTRAINT `goods_gc_fk` FOREIGN KEY (`goods_gid`) REFERENCES `shop_goods` (`goodid`) ON DELETE CASCADE,
  CONSTRAINT `user_gc_fk` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=utf8;

