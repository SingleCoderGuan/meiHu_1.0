CREATE TABLE `forum_occomment` (
  `occid` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT NULL,
  `ocid` int(11) DEFAULT NULL,
  `occontent` varchar(255) DEFAULT NULL,
  `createtime` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`occid`),
  KEY `uid_fk` (`uid`),
  KEY `off_fk` (`ocid`),
  CONSTRAINT `off_fk` FOREIGN KEY (`ocid`) REFERENCES `forum_officalarticle` (`oaid`),
  CONSTRAINT `uid_fk` FOREIGN KEY (`uid`) REFERENCES `forum_user` (`uid`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8mb4;

