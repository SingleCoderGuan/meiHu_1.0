-- ----------------------------
-- Table structure for `user_off_level`
-- ----------------------------
DROP TABLE IF EXISTS `user_off_level`;
CREATE TABLE `user_off_level` (
  `requireid` int(11) NOT NULL AUTO_INCREMENT,
  `offid` int(11) NOT NULL,
  `uid` int(11) NOT NULL,
  `remainnum` int(11) DEFAULT NULL,
  PRIMARY KEY (`requireid`),
  KEY `fku` (`offid`),
  KEY `fkd` (`uid`),
  CONSTRAINT `fkd` FOREIGN KEY (`uid`) REFERENCES `forum_user` (`uid`),
  CONSTRAINT `fku` FOREIGN KEY (`offid`) REFERENCES `off_level` (`offid`)
) ENGINE=InnoDB AUTO_INCREMENT=16 DEFAULT CHARSET=utf8;

