-- ----------------------------
-- Table structure for `shop_order`
-- ----------------------------
DROP TABLE IF EXISTS `shop_order`;
CREATE TABLE `shop_order` (
  `orderid` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) NOT NULL,
  `ordertime` date DEFAULT NULL,
  `offid` int(11) DEFAULT NULL,
  `total` double DEFAULT NULL,
  `state` varchar(4) DEFAULT NULL,
  `addressdetail` varchar(200) DEFAULT NULL,
  `receivename` varchar(60) DEFAULT NULL,
  `receivetel` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`orderid`),
  KEY `fko` (`uid`),
  KEY `fkr` (`offid`),
  CONSTRAINT `fko` FOREIGN KEY (`uid`) REFERENCES `forum_user` (`uid`),
  CONSTRAINT `fkr` FOREIGN KEY (`offid`) REFERENCES `off_level` (`offid`)
) ENGINE=InnoDB AUTO_INCREMENT=2129327095 DEFAULT CHARSET=utf8;

