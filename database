-- ----------------------------
-- Table structure for `shop_goods`
-- ----------------------------
DROP TABLE IF EXISTS `shop_goods`;
CREATE TABLE `shop_goods` (
  `goodid` int(11) NOT NULL AUTO_INCREMENT,
  `goodname` varchar(20) DEFAULT NULL,
  `goodpic` varchar(200) DEFAULT NULL,
  `goodprice` double DEFAULT NULL,
  `goodstore` int(11) DEFAULT NULL,
  `goodaddress` varchar(100) DEFAULT NULL,
  `gooddetail` varchar(200) DEFAULT NULL,
  `goodsales` int(11) DEFAULT NULL,
  `categoryid` int(11) NOT NULL,
  PRIMARY KEY (`goodid`),
  KEY `fk_1` (`categoryid`),
  CONSTRAINT `fk_1` FOREIGN KEY (`categoryid`) REFERENCES `shop_category` (`categoryid`)
) ENGINE=InnoDB AUTO_INCREMENT=10081 DEFAULT CHARSET=utf8;

