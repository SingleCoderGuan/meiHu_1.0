-- ----------------------------
-- Table structure for `forum_post`
-- ----------------------------
DROP TABLE IF EXISTS `forum_post`;
CREATE TABLE `forum_post` (
  `pid` int(20) NOT NULL AUTO_INCREMENT,
  `user_uid` int(20) NOT NULL,
  `createtime` datetime NOT NULL,
  `hot` int(1) DEFAULT '1',
  `ptitle` varchar(300) NOT NULL,
  `pcontent` varchar(20000) NOT NULL,
  `topic_tid` int(20) DEFAULT NULL,
  `likecount` int(11) DEFAULT '0',
  `visitcount` int(11) DEFAULT '0',
  PRIMARY KEY (`pid`),
  KEY `POST_USER_FK` (`user_uid`),
  KEY `POST_TOPIC_FK` (`topic_tid`),
  CONSTRAINT `POST_TOPIC_FK` FOREIGN KEY (`topic_tid`) REFERENCES `forum_topic` (`tid`),
  CONSTRAINT `POST_USER_FK` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`)
) ENGINE=InnoDB AUTO_INCREMENT=98 DEFAULT CHARSET=utf8;

