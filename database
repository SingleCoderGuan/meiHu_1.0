-- ----------------------------
-- Table structure for `forum_comment`
-- ----------------------------
DROP TABLE IF EXISTS `forum_comment`;
CREATE TABLE `forum_comment` (
  `cid` int(20) NOT NULL AUTO_INCREMENT,
  `user_uid` int(20) NOT NULL,
  `post_pid` int(20) NOT NULL,
  `commenttext` text NOT NULL,
  `commenttime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `commentid` int(11) DEFAULT NULL,
  `isread` int(11) DEFAULT '0',
  PRIMARY KEY (`cid`),
  KEY `COMMENT_USER_FK` (`user_uid`),
  KEY `COMMENT_POST_FK` (`post_pid`),
  CONSTRAINT `COMMENT_POST_FK` FOREIGN KEY (`post_pid`) REFERENCES `forum_post` (`pid`) ON DELETE CASCADE,
  CONSTRAINT `COMMENT_USER_FK` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=49 DEFAULT CHARSET=utf8;

