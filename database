-- ----------------------------
-- Table structure for `forum_postreport`
-- ----------------------------
DROP TABLE IF EXISTS `forum_postreport`;
CREATE TABLE `forum_postreport` (
  `reportid` int(20) NOT NULL AUTO_INCREMENT,
  `post_pid` int(20) NOT NULL,
  `user_uid` int(20) NOT NULL,
  `reportreason` text NOT NULL,
  `reporttime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`reportid`),
  KEY `POSTREPORT_POST_FK` (`post_pid`),
  KEY `POSTREPORT_USER_FK` (`user_uid`),
  CONSTRAINT `POSTREPORT_POST_FK` FOREIGN KEY (`post_pid`) REFERENCES `forum_post` (`pid`) ON DELETE CASCADE,
  CONSTRAINT `POSTREPORT_USER_FK` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=16 DEFAULT CHARSET=utf8;

