-- ----------------------------
-- Table structure for `forum_collection`
-- ----------------------------
DROP TABLE IF EXISTS `forum_collection`;
CREATE TABLE `forum_collection` (
  `collectionid` int(20) NOT NULL AUTO_INCREMENT,
  `user_uid` int(20) NOT NULL,
  `post_pid` int(20) NOT NULL,
  PRIMARY KEY (`collectionid`),
  KEY `COLLECTION_USER_FK` (`user_uid`),
  KEY `COLLETION_POST_FK` (`post_pid`),
  CONSTRAINT `COLLECTION_USER_FK` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE,
  CONSTRAINT `COLLETION_POST_FK` FOREIGN KEY (`post_pid`) REFERENCES `forum_post` (`pid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8;

