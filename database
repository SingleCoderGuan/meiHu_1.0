-- ----------------------------
-- Table structure for `shop_drawback`
-- ----------------------------
DROP TABLE IF EXISTS `shop_drawback`;
CREATE TABLE `shop_drawback` (
  `drawbackid` int(11) NOT NULL,
  `orderid` int(11) DEFAULT NULL,
  `itemid` int(11) DEFAULT NULL,
  `uid` int(11) DEFAULT NULL,
  `drawbackreason` varchar(200) DEFAULT NULL,
  `drawbacktime` date DEFAULT NULL,
  `detail` varchar(200) DEFAULT NULL,
  `processstate` varchar(4) DEFAULT NULL,
  PRIMARY KEY (`drawbackid`),
  KEY `fg1` (`orderid`),
  KEY `fg2` (`uid`),
  KEY `fg3` (`itemid`),
  CONSTRAINT `fg1` FOREIGN KEY (`orderid`) REFERENCES `shop_order` (`orderid`) ON DELETE CASCADE,
  CONSTRAINT `fg2` FOREIGN KEY (`uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE,
  CONSTRAINT `fg3` FOREIGN KEY (`itemid`) REFERENCES `shop_item` (`itemid`) ON DELETE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

