CREATE TABLE `forum_user` (
  `uid` int(20) NOT NULL AUTO_INCREMENT,
  `uname` varchar(20) NOT NULL,
  `password` varchar(20) NOT NULL,
  `tel` varchar(11) DEFAULT NULL,
