-- ----------------------------
-- Table structure for `shop_favor`
-- ----------------------------
DROP TABLE IF EXISTS `shop_favor`;
CREATE TABLE `shop_favor` (
  `favoriteid` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) NOT NULL,
  `goodid` int(11) NOT NULL,
  PRIMARY KEY (`favoriteid`),
  KEY `fauid` (`uid`),
  KEY `fagid` (`goodid`),
  CONSTRAINT `fagid` FOREIGN KEY (`goodid`) REFERENCES `shop_goods` (`goodid`),
  CONSTRAINT `fauid` FOREIGN KEY (`uid`) REFERENCES `forum_user` (`uid`)
) ENGINE=InnoDB AUTO_INCREMENT=25 DEFAULT CHARSET=utf8;

