-- ----------------------------
-- Table structure for `forum_commentreport`
-- ----------------------------
DROP TABLE IF EXISTS `forum_commentreport`;
CREATE TABLE `forum_commentreport` (
  `reportid` int(20) NOT NULL AUTO_INCREMENT,
  `comment_cid` int(20) NOT NULL,
  `user_uid` int(20) NOT NULL,
  `reportreason` text NOT NULL,
  `reporttime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`reportid`),
  KEY `COMMENTREPORT_COMMENT_FK` (`comment_cid`),
  KEY `COMMENTREPORT_USER_FK` (`user_uid`),
  CONSTRAINT `COMMENTREPORT_COMMENT_FK` FOREIGN KEY (`comment_cid`) REFERENCES `forum_comment` (`cid`) ON DELETE CASCADE,
  CONSTRAINT `COMMENTREPORT_USER_FK` FOREIGN KEY (`user_uid`) REFERENCES `forum_user` (`uid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8;

