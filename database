-- ----------------------------
-- Table structure for `shop_item`
-- ----------------------------
DROP TABLE IF EXISTS `shop_item`;
CREATE TABLE `shop_item` (
  `itemid` int(11) NOT NULL AUTO_INCREMENT,
  `orderid` int(11) NOT NULL,
  `goodid` int(11) NOT NULL,
  `num` int(11) DEFAULT NULL,
  `subtotal` double DEFAULT NULL,
  `item_state` int(11) DEFAULT NULL,
  PRIMARY KEY (`itemid`),
  KEY `fka` (`orderid`),
  KEY `fkb` (`goodid`),
  CONSTRAINT `fka` FOREIGN KEY (`orderid`) REFERENCES `shop_order` (`orderid`) ON DELETE CASCADE,
  CONSTRAINT `fkb` FOREIGN KEY (`goodid`) REFERENCES `shop_goods` (`goodid`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=2140548498 DEFAULT CHARSET=utf8;

